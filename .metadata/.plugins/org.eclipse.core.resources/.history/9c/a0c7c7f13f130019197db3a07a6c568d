package com.boot.startup;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.autoconfigure.domain.EntityScan;
import org.springframework.context.annotation.ComponentScan;

// http://localhost:8082/home
@SpringBootApplication
@ComponentScan(basePackages="com.jpm.lms.source.daos, com.jpm.lms.source.services, com.jpm.lms.source.controller")
@EntityScan("com.jpm.lms.source.entities")
public class ApplStart {

	public static void main(String[] args){
		SpringApplication.run(ApplStart.class, args);
	}
}
